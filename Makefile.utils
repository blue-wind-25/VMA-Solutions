# Makefile.utils

%.class: %.java
	@echo Compiling "`pwd`/$<" ...
	@CLASSPATH='$(CLASSPATH)' $(JAVAC)  -Xlint:deprecation -O $<
# --release 8
clean:
	@echo Deleting generated files in `pwd` ...
	@rm -f *.class
	@for I in $(CLEAN_DIRS); do rm -f $$I/*.class; done;
